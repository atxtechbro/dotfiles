{
  "model": "claude-opus-4-20250514",
  "cleanupPeriodDays": 90,
  "forceLoginMethod": "claudeai",
  "includeCoAuthoredBy": false,
  "additionalDirectories": ["//"],
  "permissions": {
    "defaultMode": "plan",
    "allow": [
      "LS",
      "Read",
      "Write",
      "Edit",
      "MultiEdit",
      "Glob",
      "Grep",
      "NotebookRead",
      "NotebookEdit",
      "Task",
      "TodoWrite",
      "WebFetch(domain:*)",
      "WebSearch",
      "Bash(source:*)",
      "Bash(cd:*)",
      "Bash(claude config --help)",
      "Bash(claude config get:*)",
      "Bash(claude config list:*)",
      "Bash(./utils/generate-claude-commands.sh:*)",
      "Bash(./utils/configure-claude-code-settings.sh:*)",
      "Bash(check-mcp-logs:*)",
      "Bash(python:*)",
      "mcp__git",
      "mcp__github",
      "mcp__playwright",
      "Bash(*)"
    ]
  },
  "env": {
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "8192",
    "BASH_DEFAULT_TIMEOUT_MS": "120000",
    "BASH_MAX_TIMEOUT_MS": "600000",
    "BASH_MAX_OUTPUT_LENGTH": "50000",
    "DISABLE_COST_WARNINGS": "1",
    "DISABLE_BUG_COMMAND": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "DISABLE_TELEMETRY": "1",
    "MCP_TIMEOUT": "30000",
    "MCP_TOOL_TIMEOUT": "60000",
    "MAX_MCP_OUTPUT_TOKENS": "40000"
  },
  "enableAllProjectMcpServers": false,
  "enabledMcpjsonServers": [
    "git",
    "github",
    "playwright"
  ]
}