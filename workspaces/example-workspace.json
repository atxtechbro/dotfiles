{
  "name": "Learning Environment",
  "description": "A way to learn about things you added to your dotfiles",
  "repos": [
    {
      "path": "~/tech-skills-gym",
      "name": "Tech Skills Gym",
      "branch": "main"
    },
    {
      "path": "~/dotfiles",
      "name": "Dotfiles",
      "branch": "main"
    }
  ],
  "tmux": {
    "layout": "main-vertical",
    "commands": [
      "cd ~/tech-skills-gym && ls -la",
      "cd ~/dotfiles && git status"
    ]
  },
  "environment": {
    "LEARNING_MODE": "active",
    "DOTFILES_EDITOR": "nvim"
  },
  "tools": [
    "git",
    "nvim",
    "tmux"
  ]
}
