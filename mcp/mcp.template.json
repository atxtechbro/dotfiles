{
  "mcpServers": {
    "awslabs.aws-documentation-mcp-server": {
      "command": "uvx",
      "args": ["awslabs.aws-documentation-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "git": {
      "command": "git-mcp-wrapper.sh",
      "args": [],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false
    },
    "github": {
      "command": "github-mcp-wrapper.sh",
      "args": [],
      "env": {}
    },
    "atlassian": {
      "command": "atlassian-mcp-wrapper.sh",
      "args": [],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": {{IS_PERSONAL_MACHINE}}
    },
    "gitlab": {
      "command": "gitlab-mcp-wrapper.sh",
      "args": [],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "GITLAB_READ_ONLY_MODE": "false",
        "USE_GITLAB_WIKI": "true",
        "USE_MILESTONE": "true",
        "USE_PIPELINE": "true"
      },
      "disabled": {{IS_PERSONAL_MACHINE}}
    },
    "brave-search": {
      "command": "brave-search-mcp-wrapper.sh",
      "args": [],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "filesystem": {
      "command": "filesystem-mcp-wrapper.sh",
      "args": [],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true
    },
    "gdrive": {
      "command": "gdrive-mcp-wrapper.sh",
      "args": [],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    }
  }
}