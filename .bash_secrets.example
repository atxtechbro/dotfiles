# Example secrets file - Copy to ~/.bash_secrets and fill in your values
# This file is sourced by .bashrc and should contain environment variables for API keys, tokens, etc.
# chmod 600 ~/.bash_secrets to secure this file

# AWS credentials (if not using AWS CLI profiles)
# export AWS_ACCESS_KEY_ID="your_access_key"
# export AWS_SECRET_ACCESS_KEY="your_secret_key"
# export AWS_DEFAULT_REGION="us-east-1"

# GitHub personal access token (if not using gh CLI)
# export GITHUB_TOKEN="your_github_token"

# GitLab personal access token
# export GITLAB_PERSONAL_ACCESS_TOKEN="your_gitlab_token"
# export GITLAB_API_URL="https://gitlab.com/api/v4"

# Jira credentials
# export JIRA_API_TOKEN="your_jira_api_token"
# export JIRA_USER_EMAIL="your_email@example.com"
# export JIRA_BASE_URL="https://your-domain.atlassian.net"

# Confluence credentials
# export CONFLUENCE_API_TOKEN="your_confluence_api_token"
# export CONFLUENCE_USER_EMAIL="your_email@example.com"
# export CONFLUENCE_BASE_URL="https://your-domain.atlassian.net/wiki"

# Google Maps API key
# export GOOGLE_MAPS_API_KEY="your_google_maps_api_key"

# Brave Search API key
# export BRAVE_SEARCH_API_KEY="your_brave_search_api_key"

# OpenAI API key
# export OPENAI_API_KEY="your_openai_api_key"

# Anthropic API key
# export ANTHROPIC_API_KEY="your_anthropic_api_key"

# Google Drive OAuth credentials
# export GOOGLE_DRIVE_CLIENT_ID="your_client_id"
# export GOOGLE_DRIVE_CLIENT_SECRET="your_client_secret"
# export GOOGLE_DRIVE_REFRESH_TOKEN="your_refresh_token"